[
    {
        "ConfigFile": "SCUM/Saved/Config/WindowsServer/ServerSettings.ini",
        "AutoMap": true,
        "Importable": true,
        "ConfigType": "ini",
        "ConfigFormat": "{0}={1}",
        "ConfigFormatRegex": "^(?<key>.+?)\\s*=\\s*(?<value>.*?)$"
    },
    {
        "ConfigFile": "BattlEye/BEServer_x64.cfg",
        "ConfigType": "kvp",
        "ConfigFormat": "{0} {1}",
        "Subsections": [
            {
                "Heading": "$root",
                "SettingMappings": {
                    "GameID": "scum",
                    "MasterPort": "8037",
                    "RConPassword": "$RemoteAdminPassword",
                    "RConPort": "$RCONPort",
                    "RConIP": "$ApplicationIPBinding",
                    "RestrictRCon": "0"
                }
            }
        ]
    }
]